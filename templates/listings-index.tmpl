<section class="listings-index">
  <h1>Code Listings</h1>

  <p>A collection of source code files with syntax highlighting:</p>

  <ul class="listings-list">
    {% for listing in listings %}
    <li>
      <a href="{{ listing.link }}">{{ listing.title }}</a>
    </li>
    {% endfor %}
  </ul>
</section>
