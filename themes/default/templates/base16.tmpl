{#-
  base16.tmpl - Base16 color scheme and font CSS template

  Generates CSS for Base16 color schemes and fonts:
  - Pico CSS theme variables for dark and light modes
  - Base16 palette colors (base00-base0F) for custom styles
  - Theme switcher integration
  - Google Fonts imports (if configured)
  - Font stack CSS custom properties

  Variables:
  - dark: Dark theme object with scheme_name, scheme_author, color hex values
  - light: Light theme object with scheme_name, scheme_author, color hex values
  - google_fonts_url: Google Fonts import URL (optional)
  - font_sans: Sans-serif font stack CSS
  - font_mono: Monospace font stack CSS
  - font_display: Display font stack CSS
  - font_heading: Heading font stack CSS (same as display)
  - font_emoji: Emoji font stack CSS

  Used by: Base16 feature for output/css/style.css
-#}

/* Font configuration */
{% if google_fonts_url %}
@import url("https://fonts.googleapis.com/css2?{{ google_fonts_url }}&display=swap");
{% endif %}

/* Font stacks as CSS custom properties */
:root {
    --pico-font-family: {{ font_sans }};
    --pico-font-family-sans-serif: {{ font_sans }};
    --pico-font-family-monospace: {{ font_mono }};
    --pico-font-family-emoji: {{ font_emoji }};
    --font-display: {{ font_display }};
    --font-heading: {{ font_heading }};
    font-variant-emoji: text;
}

/* Base16 color scheme integration for Nicolino
   Generated from: {{dark.scheme_name}} (dark) and {{light.scheme_name}} (light)
   Authors: {{dark.scheme_author}} / {{light.scheme_author}} */

/* Theme: {{dark.scheme_name}} */

[data-theme=dark] {
    /* Pico CSS color variables */
    --pico-background-color: #{{dark.base00_hex}};
    --pico-color: #{{dark.base05_hex}};
    --pico-text-selection-color: #{{dark.base02_hex}};
    --pico-muted-color: #{{dark.base03_hex}};
    --pico-muted-border-color: #{{dark.base02_hex}};
    --pico-primary: #{{dark.base0D_hex}};
    --pico-primary-background: #{{dark.base02_hex}};
    --pico-primary-border: var(--pico-primary-background);
    --pico-primary-underline: rgba({{dark.base0D_hex_r}}, {{dark.base0D_hex_g}}, {{dark.base0D_hex_b}}, 0.5);
    --pico-primary-hover: #{{dark.base0E_hex}};
    --pico-primary-hover-background: #{{dark.base0E_hex}};
    --pico-primary-hover-border: var(--pico-primary-hover-background);
    --pico-primary-hover-underline: var(--pico-primary-hover);
    --pico-primary-focus: rgba({{dark.base0D_hex_r}}, {{dark.base0D_hex_g}}, {{dark.base0D_hex_b}}, 0.5);
    --pico-primary-inverse: #{{dark.base00_hex}};
    --pico-secondary: #{{dark.base04_hex}};
    --pico-secondary-background: #{{dark.base02_hex}};
    --pico-secondary-border: var(--pico-secondary-background);
    --pico-secondary-underline: rgba({{dark.base04_hex_r}}, {{dark.base04_hex_g}}, {{dark.base04_hex_b}}, 0.5);
    --pico-secondary-hover: #{{dark.base05_hex}};
    --pico-secondary-hover-background: #{{dark.base03_hex}};
    --pico-secondary-hover-border: var(--pico-secondary-hover-background);
    --pico-secondary-hover-underline: var(--pico-secondary-hover);
    --pico-secondary-focus: rgba({{dark.base04_hex_r}}, {{dark.base04_hex_g}}, {{dark.base04_hex_b}}, 0.25);
    --pico-secondary-inverse: #{{dark.base00_hex}};
    --pico-contrast: #{{dark.base06_hex}};
    --pico-contrast-background: #{{dark.base06_hex}};
    --pico-contrast-border: var(--pico-contrast-background);
    --pico-contrast-underline: rgba({{dark.base06_hex_r}}, {{dark.base06_hex_g}}, {{dark.base06_hex_b}}, 0.5);
    --pico-contrast-hover: #{{dark.base07_hex}};
    --pico-contrast-hover-background: #{{dark.base07_hex}};
    --pico-contrast-hover-border: var(--pico-contrast-hover-background);
    --pico-contrast-hover-underline: var(--pico-contrast-hover);
    --pico-contrast-focus: rgba({{dark.base06_hex_r}}, {{dark.base06_hex_g}}, {{dark.base06_hex_b}}, 0.25);
    --pico-contrast-inverse: #{{dark.base00_hex}};
    --pico-box-shadow: 0 0.125rem 0.5rem rgba(0, 0, 0, 0.1);
    --pico-h1-color: #{{dark.base06_hex}};
    --pico-h2-color: #{{dark.base05_hex}};
    --pico-h3-color: #{{dark.base05_hex}};
    --pico-h4-color: #{{dark.base04_hex}};
    --pico-h5-color: #{{dark.base04_hex}};
    --pico-h6-color: #{{dark.base03_hex}};
    --pico-mark-background-color: #{{dark.base0A_hex}};
    --pico-mark-color: #{{dark.base00_hex}};
    --pico-ins-color: #{{dark.base0B_hex}};
    --pico-del-color: #{{dark.base08_hex}};
    --pico-blockquote-border-color: var(--pico-muted-border-color);
    --pico-blockquote-footer-color: var(--pico-muted-color);
    --pico-button-box-shadow: 0 0 0 rgba(0, 0, 0, 0);
    --pico-button-hover-box-shadow: 0 0 0 rgba(0, 0, 0, 0);
    --pico-table-border-color: var(--pico-muted-border-color);
    --pico-table-row-stripped-background-color: #{{dark.base01_hex}};
    --pico-code-background-color: #{{dark.base01_hex}};
    --pico-code-color: #{{dark.base0A_hex}};
    --pico-code-kbd-background-color: var(--pico-color);
    --pico-code-kbd-color: var(--pico-background-color);
    --pico-form-element-background-color: #{{dark.base00_hex}};
    --pico-form-element-selected-background-color: #{{dark.base02_hex}};
    --pico-form-element-border-color: #{{dark.base03_hex}};
    --pico-form-element-color: #{{dark.base05_hex}};
    --pico-form-element-placeholder-color: var(--pico-muted-color);
    --pico-form-element-active-background-color: #{{dark.base00_hex}};
    --pico-form-element-active-border-color: var(--pico-primary-border);
    --pico-form-element-focus-color: var(--pico-primary-border);
    --pico-form-element-disabled-opacity: 0.5;
    --pico-form-element-invalid-border-color: #{{dark.base08_hex}};
    --pico-form-element-invalid-active-border-color: #{{dark.base09_hex}};
    --pico-form-element-invalid-focus-color: var(--pico-form-element-invalid-active-border-color);
    --pico-form-element-valid-border-color: #{{dark.base0B_hex}};
    --pico-form-element-valid-active-border-color: #{{dark.base0C_hex}};
    --pico-form-element-valid-focus-color: var(--pico-form-element-valid-active-border-color);
    --pico-switch-background-color: #{{dark.base03_hex}};
    --pico-switch-checked-background-color: var(--pico-primary-background);
    --pico-switch-color: #{{dark.base00_hex}};
    --pico-switch-thumb-box-shadow: 0 0 0 rgba(0, 0, 0, 0);
    --pico-range-border-color: #{{dark.base02_hex}};
    --pico-range-active-border-color: #{{dark.base03_hex}};
    --pico-range-thumb-border-color: var(--pico-background-color);
    --pico-range-thumb-color: var(--pico-secondary-background);
    --pico-range-thumb-active-color: var(--pico-primary-background);
    --pico-accordion-border-color: var(--pico-muted-border-color);
    --pico-accordion-active-summary-color: var(--pico-primary-hover);
    --pico-accordion-close-summary-color: var(--pico-color);
    --pico-accordion-open-summary-color: var(--pico-muted-color);
    --pico-card-background-color: #{{dark.base00_hex}};
    --pico-card-border-color: var(--pico-muted-border-color);
    --pico-card-box-shadow: var(--pico-box-shadow);
    --pico-card-sectioning-background-color: #{{dark.base01_hex}};
    --pico-dropdown-background-color: #{{dark.base00_hex}};
    --pico-dropdown-border-color: #{{dark.base02_hex}};
    --pico-dropdown-box-shadow: var(--pico-box-shadow);
    --pico-dropdown-color: var(--pico-color);
    --pico-dropdown-hover-background-color: #{{dark.base01_hex}};
    --pico-loading-spinner-opacity: 0.5;
    --pico-modal-overlay-background-color: rgba(0, 0, 0, 0.5);
    --pico-progress-background-color: #{{dark.base02_hex}};
    --pico-progress-color: var(--pico-primary-background);
    --pico-tooltip-background-color: var(--pico-contrast-background);
    --pico-tooltip-color: var(--pico-contrast-inverse);

    /* Base16 palette colors for use in custom styles */
    --b16-base00: #{{dark.base00_hex}}; /* Default background */
    --b16-base01: #{{dark.base01_hex}}; /* Lighter background */
    --b16-base02: #{{dark.base02_hex}}; /* Selection background */
    --b16-base03: #{{dark.base03_hex}}; /* Comments, secondary */
    --b16-base04: #{{dark.base04_hex}}; /* Darker foreground */
    --b16-base05: #{{dark.base05_hex}}; /* Default foreground */
    --b16-base06: #{{dark.base06_hex}}; /* Lighter foreground */
    --b16-base07: #{{dark.base07_hex}}; /* Lightest foreground */
    --b16-base08: #{{dark.base08_hex}}; /* Variables, etc */
    --b16-base09: #{{dark.base09_hex}}; /* Integers, constants */
    --b16-base0A: #{{dark.base0A_hex}}; /* Classes, search match */
    --b16-base0B: #{{dark.base0B_hex}}; /* Strings */
    --b16-base0C: #{{dark.base0C_hex}}; /* Support, regex */
    --b16-base0D: #{{dark.base0D_hex}}; /* Functions, methods */
    --b16-base0E: #{{dark.base0E_hex}}; /* Keywords */
    --b16-base0F: #{{dark.base0F_hex}}; /* Builtin, punctuation */
}

