/* Highlight.js syntax highlighting - dark theme */
pre code.hljs,
[data-theme=dark] pre code.hljs,
[data-theme=dark] .hljs {
    color: #{{dark.base05_hex}};
    background: #{{dark.base01_hex}};
}
[data-theme=dark] .hljs-comment,
[data-theme=dark] .hljs-quote {
    color: #{{dark.base03_hex}};
    font-style: italic;
}
[data-theme=dark] .hljs-tag,
[data-theme=dark] .hljs-variable,
[data-theme=dark] .hljs-template-variable,
[data-theme=dark] .hljs-selector-tag,
[data-theme=dark] .hljs-name,
[data-theme=dark] .hljs-deletion {
    color: #{{dark.base08_hex}};
}
[data-theme=dark] .hljs-symbol,
[data-theme=dark] .hljs-number,
[data-theme=dark] .hljs-link,
[data-theme=dark] .hljs-attr,
[data-theme=dark] .hljs-variable.constant_,
[data-theme=dark] .hljs-literal {
    color: #{{dark.base09_hex}};
}
[data-theme=dark] .hljs-title,
[data-theme=dark] .hljs-class .hljs-title,
[data-theme=dark] .hljs-title.class_ {
    color: #{{dark.base0A_hex}};
}
[data-theme=dark] .hljs-strong {
    font-weight: bold;
    color: #{{dark.base0A_hex}};
}
[data-theme=dark] .hljs-code,
[data-theme=dark] .hljs-addition,
[data-theme=dark] .hljs-title.class_.inherited__,
[data-theme=dark] .hljs-string {
    color: #{{dark.base0B_hex}};
}
[data-theme=dark] .hljs-built_in,
[data-theme=dark] .hljs-doctag,
[data-theme=dark] .hljs-quote,
[data-theme=dark] .hljs-keyword.hljs-atrule,
[data-theme=dark] .hljs-regexp {
    color: #{{dark.base0C_hex}};
}
[data-theme=dark] .hljs-function .hljs-title,
[data-theme=dark] .hljs-attribute,
[data-theme=dark] .hljs-title.function_ {
    color: #{{dark.base0D_hex}};
}
[data-theme=dark] .hljs-type,
[data-theme=dark] .hljs-template-tag,
[data-theme=dark] .diff .hljs-meta,
[data-theme=dark] .hljs-keyword {
    color: #{{dark.base0E_hex}};
}
[data-theme=dark] .hljs-emphasis {
    color: #{{dark.base0E_hex}};
    font-style: italic;
}
[data-theme=dark] .hljs-meta,
[data-theme=dark] .hljs-meta .hljs-keyword,
[data-theme=dark] .hljs-meta .hljs-string {
    color: #{{dark.base0F_hex}};
}

